###############################################################################
# Matlab/Octave language
###############################################################################
# Windows default autosave extension
/(matlab|octave)/**/.asv
# OSX / *nix default autosave extension
/(matlab|octave)/**/.m~
# Compiled MEX binaries (all platforms)
/(matlab|octave)/**/.mex*
# Packaged app and toolbox files
/(matlab|octave)/**/.mlappinstall
/(matlab|octave)/**/.mltbx
# Generated helpsearch folders
/(matlab|octave)/**/elpsearch*/
# Simulink code generation folders
/(matlab|octave)/**/lprj/
/(matlab|octave)/**/ccprj/
# Matlab code generation folders
/(matlab|octave)/**/odegen/
# Simulink autosave extension
/(matlab|octave)/**/.autosave
# Simulink cache files
/(matlab|octave)/**/.slxc
# Octave session info
/(matlab|octave)/**/ctave-workspace

###############################################################################
# Python language
###############################################################################
# Byte-compiled / optimized / DLL files
/python/**/__pycache__/
/python/**/*.py[cod]
/python/**/*$py.class
# C extensions
/python/**/*.so
# Distribution / packaging
/python/**/.Python
/python/**/build/
/python/**/develop-eggs/
/python/**/dist/
/python/**/downloads/
/python/**/eggs/
/python/**/.eggs/
/python/**/lib/
/python/**/lib64/
/python/**/parts/
/python/**/sdist/
/python/**/var/
/python/**/wheels/
/python/**/share/python-wheels/
/python/**/*.egg-info/
/python/**/.installed.cfg
/python/**/*.egg
/python/**/MANIFEST
# PyInstaller
#  Usually these files are written by a python script from a template
#  before PyInstaller builds the exe, so as to inject date/other infos into it.
/python/**/*.manifest
/python/**/*.spec
# Installer logs
/python/**/pip-log.txt
/python/**/pip-delete-this-directory.txt
# Unit test / coverage reports
/python/**/htmlcov/
/python/**/.tox/
/python/**/.nox/
/python/**/.coverage
/python/**/.coverage.*
/python/**/.cache
/python/**/nosetests.xml
/python/**/coverage.xml
/python/**/*.cover
/python/**/*.py,cover
/python/**/.hypothesis/
/python/**/.pytest_cache/
/python/**/cover/
# Translations
/python/**/*.mo
/python/**/*.pot
# Django stuff:
/python/**/*.log
/python/**/local_settings.py
/python/**/db.sqlite3
/python/**/db.sqlite3-journal
# Flask stuff:
/python/**/instance/
/python/**/.webassets-cache
# Scrapy stuff:
/python/**/.scrapy
# Sphinx documentation
/python/**/docs/_build/
# PyBuilder
/python/**/.pybuilder/
/python/**/target/
# Jupyter Notebook
/python/**/.ipynb_checkpoints
# IPython
/python/**/profile_default/
/python/**/ipython_config.py
# PEP 582; used by e.g. github.com/David-OConnor/pyflow
/python/**/__pypackages__/
# Celery stuff
/python/**/celerybeat-schedule
/python/**/celerybeat.pid
# SageMath parsed files
/python/**/*.sage.py
# Environments
/python/**/.env
/python/**/.venv
/python/**/env/
/python/**/venv/
/python/**/ENV/
/python/**/env.bak/
/python/**/venv.bak/
# Spyder project settings
/python/**/.spyderproject
/python/**/.spyproject
# Rope project settings
/python/**/.ropeproject
# mkdocs documentation
/python/site
# mypy
/python/**/.mypy_cache/
/python/**/.dmypy.json
/python/**/dmypy.json
# Pyre type checker
/python/**/.pyre/
# pytype static type analyzer
/python/**/.pytype/
# Cython debug symbols
/python/**/cython_debug/

###############################################################################
# R language
###############################################################################
# History files
/r/**/.Rhistory
/r/**/.Rapp.history
# Session Data files
/r/**/.RData
/r/**/.RDataTmp
# User-specific files
/r/**/.Ruserdata
# Example code in package build process
/r/**/*-Ex.R
# Output files from R CMD build
/r/*.tar.gz
# Output files from R CMD check
/r/*.Rcheck/
# RStudio files
/r/**/.Rproj.user/
# produced vignettes
/r/**/vignettes/*.html
/r/**/vignettes/*.pdf
# OAuth2 token, see https://github.com/hadley/httr/releases/tag/v0.3
/r/**/.httr-oauth
# knitr and R markdown default cache directories
/r/**/*_cache/
/r/cache/
# Temporary files created by R markdown
/r/**/*.utf8.md
/r/**/*.knit.md
# R Environment Variables
/r/**/.Renviron
# pkgdown site
/r/**/docs/
# translation temp files
/r/**/po/*~
# RStudio Connect folder
/r/**/rsconnect/
